-- Loaded Anti Lagg V1 --
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local renderDistance = 200 -- กำหนดระยะการแสดงผล
-- ฟังก์ชันตรวจสอบระยะห่างและจัดการกับต้นไม้
local function checkTrees()
    for _, obj in pairs(workspace:GetChildren()) do
        if obj:IsA("Model") and obj:FindFirstChild("TreeTag") then -- ตรวจสอบว่าคือ Model และเป็นต้นไม้
            local distance = (obj.PrimaryPart.Position - character.HumanoidRootPart.Position).Magnitude
            if distance > renderDistance then
                obj.Parent = nil -- ลบต้นไม้ออกจาก workspace
            else
                obj.Parent = workspace -- โหลดต้นไม้กลับเข้ามาใน workspace
            end
        end
    end
end

-- ทำการเช็คระยะห่างทุก ๆ 1 วินาที
while wait(1) do
    checkTrees()
end

  -- Check Join Player --
local Players = game:GetService("Players")
local ChatService = game:GetService("Chat")

-- ฟังก์ชันที่จะแสดงข้อความในช่องแชทเมื่อมีผู้เล่นเข้าร่วม
local function announcePlayerJoin(player)
    ChatService:Chat(player.Character or player.CharacterAdded:Wait(), player.Name .. " has joined the game!", Enum.ChatColor.Green)
end

-- ตรวจจับผู้เล่นที่เข้ามาตอนเริ่มเกม
for _, player in pairs(Players:GetPlayers()) do
    announcePlayerJoin(player)
end

-- ตรวจจับผู้เล่นใหม่ที่เข้ามาหลังจากเริ่มเกม
Players.PlayerAdded:Connect(announcePlayerJoin)

